					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"var template = `\r",
									"    <table bgcolor=\"#FFFFFF\">\r",
									"        <tr>\r",
									"            <th>Id</th>\r",
									"            <th>Title</th>\r",
									"            <th>Author</th>\r",
									"            <th>Description</th>\r",
									"            <th>CreatedAt</th>\r",
									"            <th>UpdateAt</th>\r",
									"            <th>Image</th>\r",
									"            <th>Subtitle</th>\r",
									"            <th>Pulisher</th>\r",
									"            <th>Isbn13</th>\r",
									"            <th>Price</th>\r",
									"            <th>Year</th>\r",
									"            <th>Rating</th>\r",
									"            <th>UserId</th>\r",
									"        </tr>\r",
									"        {{#each response}}\r",
									"            <tr>\r",
									"                <td>{{id}}</td>\r",
									"                <td>{{title}}</td>\r",
									"                <td>{{author}}</td>\r",
									"                <td class=\"description\">{{description}}</td>\r",
									"                <td>{{createdAt}}</td>\r",
									"                <td>{{updatedAt}}</td>\r",
									"                <td><img src=\"{{image}}\" alt=\"{{title}}\" width=\"100\" height=\"200\"></td>\r",
									"                <td>{{subtitle}}</td>                \r",
									"                <td>{{publisher}}</td>\r",
									"                <td>{{isbn13}}</td>\r",
									"                <td>{{price}}</td>\r",
									"                <td>{{year}}</td>\r",
									"                <td>{{rating}}</td>\r",
									"                <td>{{userId}}</td>\r",
									"            </tr>\r",
									"        {{/each}}\r",
									"    </table>\r",
									"\r",
									"    <style>\r",
									"        td.description {\r",
									"            max-width: 200px;\r",
									"            white-space: nowrap;\r",
									"            overflow: hidden;\r",
									"            text-overflow: ellipsis;\r",
									"        }\r",
									"    </style>\r",
									"`;\r",
									"\r",
									"pm.visualizer.set(template, {\r",
									"    response: pm.response.json()\r",
									"});"
								],
